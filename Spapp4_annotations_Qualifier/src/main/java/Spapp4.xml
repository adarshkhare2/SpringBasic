<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<context:component-scan
		base-package="Spapp4.xml" />
	<context:annotation-config />

	<bean id="student" class="com.Spapp4.Student">
		<property name="rollno" value="1954" />
		<property name="persentage" value="94.50" />
		<property name="name" value="Adarsh" />
	</bean>

	<bean id="dateOfBrithBean" class="com.Spapp4.DateOfBrithBean">
		<property name="day" value="25" />
		<property name="month" value="March" />
		<property name="year" value="2000" />
	</bean>

	<bean id="dateOfBrithBean1" class="com.Spapp4.DateOfBrithBean">
		<property name="day" value="12" />
		<property name="month" value="Sep" />
		<property name="year" value="2002" />
	</bean>

</beans>